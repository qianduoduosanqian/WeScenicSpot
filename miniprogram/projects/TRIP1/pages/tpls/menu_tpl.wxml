<template name="menuTpl"> 
	<view class="tab-bar" >   
		<view wx:if="{{returnHome}}" class="tab-bar-home" bindtap="url" data-type="relaunch"
			data-url="/pages/home/index/home_index">
			<text class="tab-bar-home-text icon-homefill text-grey"></text>
		</view>

		<view class="tab-bar-border"></view>
		<view class="tab-bar-item" bindtap="url" data-type="relaunch" data-url="/projects/TRIP1/pages/default/index/default_index">
			<image class="tab-icon" src="/projects/TRIP1/images/tabbar/home{{curMenu=='home_index'?'_cur':''}}.png"></image>
			<view class="tab-text {{curMenu=='home_index'?'menu-project':''}}">首页</view>
		</view>

		<view class="tab-bar-item" bindtap="url" data-type="relaunch" data-url="/projects/TRIP1/pages/product/index/product_index">
			<image class="tab-icon" src="/projects/TRIP1/images/tabbar/jing{{curMenu=='product_index'?'_cur':''}}.png"></image>
			<view class="tab-text {{curMenu=='product_index'?'menu-project':''}}">景点</view>
		</view>

		<view wx:if="{{skin.cate2!==false}}" class="tab-bar-item" bindtap="url" data-type="relaunch" data-url="/projects/TRIP1/pages/album/index/album_index">
			<image class="tab-icon" src="/projects/TRIP1/images/tabbar/gong{{curMenu=='album_index'?'_cur':''}}.png"></image>
			<view class="tab-text {{curMenu=='album_index'?'menu-project':''}}">攻略</view>
		</view>
		 
		<view class="tab-bar-item" bindtap="url" data-type="relaunch" data-url="/projects/TRIP1/pages/about/service/about_service">
			<image class="tab-icon" src="/projects/TRIP1/images/tabbar/service{{curMenu=='service'?'_cur':''}}.png"></image>
			<view class="tab-text {{curMenu=='service'?'menu-project':''}}">服务</view>
		</view>
		<view class="tab-bar-item" bindtap="url" data-type="relaunch" data-url="/projects/TRIP1/pages/my/index/my_index">
			<image class="tab-icon" src="/projects/TRIP1/images/tabbar/my{{curMenu=='my_index'?'_cur':''}}.png"></image>
			<view class="tab-text {{curMenu=='my_index'?'menu-project':''}}">我的</view>
		</view>


	</view>
</template>